{
  "type": "sqlite",
  "database": "data/eth.sqlite",
  "synchronize": true,
  "logging": false,
  "entities": [
    "src/entities/*.ts"
  ],
  "subscribers": [
    "src/subscribers/*.ts"
  ],
  "migrations": [
    "src/migrations/*.ts"
  ],
  "migrationsTableName": "migrations",
  "cli": {
    "entitiesDir": "src/entities",
    "migrationsDir": "./src/migrations",
    "subscribersDir": "src/subscribers"
  }
}